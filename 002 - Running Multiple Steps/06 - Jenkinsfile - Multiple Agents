pipeline {
        agent {
            docker {
                image 'maven:3.3.3'
            }
        }
    stages {


            stage('mvn') {
                steps {
                    sh 'mvn --version'
                }
            }

            agent {
                docker {
                    image 'maven:3.3.3'
                }
            }
            stage('npm') {
                steps {
                    sh 'npm --version'
                }
            }
    }
}